{
  "name": "vwo-node-sdk-example",
  "version": "1.0.0",
  "description": "VWO Node SDK example",
  "main": "index.js",
  "scripts": {
    "dev-app": "nodemon --inspect=0.0.0.0:9229 --legacy-watch --ignore node_modules index.js",
    "lint": "eslint '**/*.js' --fix --cache",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vwo",
    "node-sdk",
    "SDK"
  ],
  "author": "dev@wingify.com",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "vwo-node-sdk": "^0.1.0-alpha.7"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "globOptions": {
      "matchBase": false
    },
    "linters": {
      "**/*.js": [
        "prettier",
        "git add"
      ],
      "**/*.{json,md}": [
        "prettier",
        "git add"
      ]
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 6.10.0"
  }
}
