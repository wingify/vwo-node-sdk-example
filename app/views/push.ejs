<% include header.ejs%>

<div>
  <h2 class="center  color-blue">VWO Node SDK Example</h2>
  <div class="main-container">
    <div class="left-container">
      <h2 class="center  color-grey">Settings</h2>
      <pre><code><%- currentSettingsFile %></code></pre>
    </div>

    <div class="right-container">
      <div class="half-containers">
        <h2 class="center  color-grey">Details</h2>

        <div class="center  margin--top">
          <div>Tag key - <strong><%=customDimensionKey %></strong></div>
        </div>

        <div class="center margin--top">
            <div>Tag value - <strong><%= customDimensionValue%></strong></div>
        </div>

        <div class="center  margin--top">
          <div>userId - <strong><%=userId %></strong></div>
        </div>

        <%if (result) { %>
            <div class="center  margin--top">
              <div>Data pushed to server successfully</div>
            </div>
        <% } else { %>
            <div class="center  margin--top">
                <div>Failed to push data to the server</div>
            </div>
        <% } %>

      </div>
      <div class="half-containers  center">
        <button onclick="window.location.reload()">Refresh</button>
      </div>

    </div>
  </div>

</div>

<% include footer.ejs%>
